<?xml version="1.0" ?>

<!-- Please see syssvc.xsd for the format of this file -->
<services noNamespaceSchemaLocation="syssvc.xsd">

  <svc name="IopCreateDevice">
    <parameter annotation="in opt" type="UnicodeString" name="DeviceName" />
    <parameter annotation="in" type="DEVICE_TYPE" name="DeviceType" />
    <parameter annotation="in" type="ULONG" name="DeviceCharacteristics" />
    <parameter annotation="in" type="BOOLEAN" name="Exclusive" />
    <parameter annotation="out" type="ptr::HANDLE" name="DeviceHandle" />
  </svc>

  <svc name="IopRequestIrp">
    <parameter annotation="out" type="ptr::ULONG" name="NumIrp" />
  </svc>

</services>
